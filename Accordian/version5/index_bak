        <script type='text/javascript'>

        $(document).ready(function() {

            $('.closeall').click(function(){
  $('.panel-collapse.in')
    .collapse('hide');
});
$('.openall').click(function(){
  $('.panel-collapse:not(".in")')
    .collapse('show');
});

        });

        </script>

